{
  "name": "hifi_1_batch_config",
  "n_gpu": 1,
  "overfit_on_one_batch": "False",
  "preprocessing": {
    "sr": 22050,
    "spectrogram": {
      "type": "MelSpectrogram",
      "args": {
        "n_mels": 80,
        "win_length": 1024,
        "hop_length": 256,
        "n_fft": 1024,
        "f_min": 0,
        "f_max": 8000,
        "power": 1.0,
        "pad": 384
      }
    }
  },
  "archG": {
    "type": "Generator",
    "args": {
      "hidden_ch": 512,
      "k_u": [16, 16, 4, 4],
      "k_r": [3, 7, 11],
      "d_r": [[1, 1], [3, 1], [5, 1]],
      "d_r_repeat": 3
    }
  },
  "archD": {
    "type": "Discriminator",
    "args": {
      "periods": [2, 3, 5, 7, 11]
    }
  },
  "data": {
    "all": {
      "batch_size": 16,
      "num_workers": 5,
      "test_size": 1,
      "datasets": [
        {
          "type": "LJSpeechDataset",
          "args": {
            "download": "False",
            "segment_size": 8192,
            "data_dir": "/home/jupyter/mnt/s3/bucket-hse-rw/data/datasets"
          }
        }
      ]
    }
  },
  "optimizerG": {
    "type": "AdamW",
    "args": {
      "lr": 2e-4,
      "betas": [0.8, 0.99],
      "weight_decay": 0.01
    }
  },
  "optimizerD": {
    "type": "AdamW",
    "args": {
      "lr": 2e-4,
      "betas": [0.8, 0.99],
      "weight_decay": 0.01
    }
  },
  "lr_schedulerG": {
    "use": "True",
    "type": "ExponentialLR",
    "frequency": "epoch",
    "args": {
      "gamma": 0.999
    }
  },
  "lr_schedulerD": {
    "use": "True",
    "type": "ExponentialLR",
    "frequency": "epoch",
    "args": {
      "gamma": 0.999
    }
  },
  "loss": {
    "type": "HiFiLoss",
    "args": {
      "lam_fm": 2,
      "lam_mel": 45
    }
  },
  "trainer": {
    "epochs": 3100,
    "save_dir": "saved/",
    "save_period": 1,
    "verbosity": 2,
    "wandb_project": "hifi",
    "log_step": 200
  }
}
