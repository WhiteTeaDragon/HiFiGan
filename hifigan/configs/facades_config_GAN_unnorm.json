{
  "name": "pix2pix_config",
  "n_gpu": 1,
  "overfit_on_one_batch": "False",
  "archG": {
    "type": "Generator",
    "args": {
      "input_channels": 3,
      "output_channels": 3,
      "hidden_channels": 64,
      "factors": [2, 4, 8, 8, 8, 8, 8],
      "num_of_dropouts": 3
    }
  },
  "archD": {
    "type": "Discriminator",
    "args": {
      "input_ch": 3,
      "output_ch": 3,
      "hidden_ch": 64
    }
  },
  "data": {
    "train": {
      "batch_size": 1,
      "num_workers": 5,
      "datasets": [
        {
          "type": "ImageToImageDataset",
          "args": {
            "name": "facades",
            "augs": "True"
          }
        }
      ]
    },
    "val": {
      "batch_size": 1,
      "num_workers": 5,
      "datasets": [
        {
          "type": "ImageToImageDataset",
          "args": {
            "name": "facades"
          }
        }
      ]
    },
    "test": {
      "batch_size": 1,
      "num_workers": 5,
      "datasets": [
        {
          "type": "ImageToImageDataset",
          "args": {
            "name": "facades"
          }
        }
      ]
    }
  },
  "optimizerG": {
    "type": "Adam",
    "args": {
      "lr": 2e-4,
      "betas": [0.5, 0.999]
    }
  },
  "optimizerD": {
    "type": "Adam",
    "args": {
      "lr": 2e-4,
      "betas": [0.5, 0.999]
    }
  },
  "loss": {
    "type": "Pix2PixLoss",
    "args": {
      "lam": 100
    }
  },
  "trainer": {
    "epochs": 200,
    "save_dir": "saved/",
    "save_period": 50,
    "verbosity": 2,
    "wandb_project": "pix2pix",
    "log_step": 200,
    "fid_log_step": 10
  }
}
